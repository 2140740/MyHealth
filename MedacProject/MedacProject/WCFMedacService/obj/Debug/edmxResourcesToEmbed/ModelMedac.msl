        </MappingFragment>
      </EntityTypeMapping>
    </EntitySetMapping>
    <AssociationSetMapping Name="PatientMeasurement" TypeName="ModelMedac.PatientMeasurement" StoreEntitySet="MeasurementSet">
      <EndProperty Name="Patient">
        <ScalarProperty Name="Id" ColumnName="Patient_Id" />
      </EndProperty>
      <EndProperty Name="Measurement">
        <ScalarProperty Name="Id" ColumnName="Id" />
      </EndProperty>
    </AssociationSetMapping>
    <AssociationSetMapping Name="DoctorPatient" TypeName="ModelMedac.DoctorPatient" StoreEntitySet="PatientSet">
      <EndProperty Name="Doctor">
        <ScalarProperty Name="Id" ColumnName="Doctor_Id" />
